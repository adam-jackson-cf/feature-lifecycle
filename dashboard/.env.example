# Database
DATABASE_PATH=data/lifecycle.db

# Jira Integration (required for importing Jira data)
JIRA_HOST=https://your-instance.atlassian.net
JIRA_EMAIL=your-email@example.com
JIRA_API_TOKEN=your-api-token

# GitHub Integration (optional - for higher rate limits and private repos)
# Create a token at: https://github.com/settings/tokens
GITHUB_TOKEN=

# Seed Data (set to true to enable seed data generation)
# Run: bun run seed
ENABLE_SEED_DATA=false
